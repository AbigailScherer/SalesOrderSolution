@model SalesOrdersProject.Models.Customer
@{
    ViewBag.Title = "Index";
}
<link href="~/Content/Site.css" rel="stylesheet" />
<title>Sign in</title>
<body class="body">
    <div class="main">
        <p class="sign" align="center">Sign in</p>
        <form class="form1">
            <input class="un " type="text" align="center" placeholder="Username">
